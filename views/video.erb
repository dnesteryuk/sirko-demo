<div class="container mtb">
  <% random_youtube_codes.each do |youtube_code| %>
    <div class="row">
      <div class="col-md-6">
        <iframe width="600" height="400" src="https://www.youtube.com/embed/<%= youtube_code %>" frameborder="0" allowfullscreen></iframe>
      </div>
      <div class="col-md-5 col-md-offset-1">
        <h3><%= Faker::Lorem.sentence %></h3>
        <p><%= Faker::Lorem.paragraph %></p>
      </div>
    </div>
  <% end %>
</div>

<%= erb 'partials/footer'.to_sym %>
<%= erb 'partials/scripts'.to_sym %>
